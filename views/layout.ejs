<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Name App</title>
    <link rel="stylesheet" type="text/css" href="css/nameapp.css" media="screen">
</head>
<body>

<%- body %>

<table id="employees">
    <thead>
        <tr>
            <th>Name</th>
            <th>Amount</th>
        </tr>
    </thead>
    <tbody>
        <% nameData.names.forEach(function(item){ %>
        <tr>
            <td>
                <%= item.name %>
            </td>
            <td>
                <%= item.amount %>
            </td>
        </tr>
        <%}) %>
    </tbody>

</table>

</body>
</html>